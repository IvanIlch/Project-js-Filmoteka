{{#each this}}
<li class="list-films__item">
  <div class="foto-box">
    <img src="https://image.tmdb.org/t/p/w500/{{poster_path}}" alt="{{original_title}}" data-id="{{id}}" class="foto-img" />
  </div>
  <p class="list-films__item-text">{{original_title}}</p>
  <div class="raiting-box">
    <ul class="list-films__item-ganre-list">
      {{!-- {{#each genre_ids}}
      <li class="list-films__item-ganre-list-item"><span>{{this}}</span></li>>
      {{/each}} --}}
    </ul>
    <p class="list-films__item-jear"><span class="list-films__item-jear-span">|{{release_date}}</span></p>
    <p class="list-films__item-rating">{{vote_average}}</p>
  </div>
</li>
{{/each}}